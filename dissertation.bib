@report{akesson_fatalism_2020,
  title = {Fatalism, {{Beliefs}}, and {{Behaviors During}} the {{COVID}}-19 {{Pandemic}}},
  author = {Akesson, Jesper and Ashworth-Hayes, Sam and Hahn, Robert and Metcalfe, Robert D and Rasooly, Itzhak},
